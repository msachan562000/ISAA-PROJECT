<body>

<h1>Add Customer</h1>
<form class="" action="/addnewcustomer" method="post">
  <div class="form-group">
<label>First Name:</label>
<input type="text" class="form-control"name="fname" autocomplete="off">
<label>Last Name:</label>
<input type="text" name="lname" rows="5" class="form-control"></input>
<label>Age:</label>
<input type="number" name="age" rows="5" class="form-control"></input>
<label>Email-ID:</label>
<input type="email" name="email" rows="5" class="form-control"></input>
<label>Account Number:</label>
<input type="text" name="account" rows="5" class="form-control"></input>
<label>IFSC Code:</label>
<input type="text" name="ifsc" rows="5" class="form-control"></input>
<label>Available Balance:</label>
<input type="text" name="balance" rows="5" class="form-control"></input>

  </div>
  <button type="submit"class="btn btn-primary" name="pub">Add Customer</button>
</form>

<script src="/docs/5.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>
